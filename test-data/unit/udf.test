@pandas_udf("int", PandasUDFType.GROUPED_AGG)
def h(*xs):
    return 42

pandas_udf(lambda x: 42, "str", PandasUDFType.GROUPED_AGG)
pandas_udf(lambda x, y: 42, "str", PandasUDFType.GROUPED_AGG)
pandas_udf(lambda *xs: 42, "str", PandasUDFType.GROUPED_AGG)
